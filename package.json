{
  "name": "@dontuncleme/lint",
  "version": "1.0.5",
  "description": "Set of configs for linters",
  "author": "Siarhiej Samsona≈≠ <dontuncleme@gmail.com>",
  "license": "MIT",
  "keywords": [
    "lint",
    "eslint",
    "stylelint",
    "typescript",
    "scss"
  ],
  "scripts": {
    "editorconfig": "editorconfig-checker"
  },
  "lint-staged": {
    "**/*.{js,yml}": [
      "npx prettier --write"
    ]
  },
  "prettier": "@dontuncleme/prettier",
  "simple-git-hooks": {
    "pre-commit": "npx lint-staged"
  },
  "dependencies": {
    "@typescript-eslint/eslint-plugin": "^4.31.1",
    "@typescript-eslint/parser": "^4.31.1",
    "eslint": "^7.32.0",
    "eslint-plugin-ascii": "^1.0.0",
    "eslint-plugin-import": "^2.24.2",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "stylelint": "^13.13.1",
    "stylelint-order": "^4.1.0",
    "stylelint-scss": "^3.21.0",
    "typescript": "^4.4.3"
  },
  "devDependencies": {
    "@dontuncleme/prettier": "^1.0.1",
    "editorconfig-checker": "^4.0.2",
    "lint-staged": "^11.1.2",
    "simple-git-hooks": "^2.6.1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dontuncleme/lint.git"
  },
  "bugs": {
    "url": "https://github.com/dontuncleme/lint/issues"
  },
  "homepage": "https://github.com/dontuncleme/lint#readme"
}
